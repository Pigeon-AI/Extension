{
  "manifest_version": 3,
  "name": "Pigeon AI",
  "version": "0.1",
  "description": "Pigeon AI chromium extension",
  "background": {
    "service_worker": "static/js/background.js"
  },
  "permissions": [
    "unlimitedStorage",
    "activeTab",
    "tabs",
    "scripting",
    "tts"
  ],
  "host_permissions": [
    "<all_urls>",
    "*://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "React Chrome Extension",
    "default_icon": {
      "16": "/images/pidgey_16.png",
      "32": "/images/pidgey_32.png",
      "48": "/images/pidgey_48.png",
      "128": "/images/pidgey_128.png"
    }
  },
  "icons": {
    "16": "/images/pidgey_16.png",
    "32": "/images/pidgey_32.png",
    "48": "/images/pidgey_48.png",
    "128": "/images/pidgey_128.png"
  },
  "options_ui": {
    "page": "options.html"
  }
}
